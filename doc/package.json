{
  "name": "@ignavia/ella",
  "version": "1.0.4",
  "description": "Linear algebra stuff.",
  "keywords": [
    "linear-algebra",
    "math",
    "maths"
  ],
  "bugs": "https://github.com/Ignavia/js-ella/issues",
  "license": "MIT",
  "author": "Lars Reimann <mail@lars-reimann.eu> (http://lars-reimann.eu)",
  "files": [
    "dist/"
  ],
  "main": "dist/ella.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ignavia/js-ella.git"
  },
  "scripts": {
    "build": "jspm build ./src/ella.js ./dist/ella.js --format umd --global-name \"IGNAVIA_ELLA\" --global-deps \"{}\" --skip-source-maps --minify",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover --report text --report html node_modules/mocha/bin/_mocha -- --reporter dot",
    "doc": "esdoc -c esdoc.json",
    "prepublish": "npm run build",
    "test": "mocha --compilers js:babel-register"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.5.1",
    "chai": "^3.5.0",
    "esdoc": "^0.4.4",
    "isparta": "^4.0.0",
    "jspm": "^0.17.0-beta.6",
    "mocha": "^2.4.5"
  },
  "jspmPackage": true,
  "registry": "npm",
  "format": "cjs",
  "jspm": {
    "name": "@ignavia/ella",
    "dependencies": {},
    "devDependencies": {
      "plugin-babel": "systemjs-plugin-babel@^0.0.2"
    },
    "peerDependencies": {
      "assert": "github:jspm/nodelibs-assert@^0.2.0-alpha",
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "child_process": "github:jspm/nodelibs-child_process@^0.2.0-alpha",
      "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "http": "github:jspm/nodelibs-http@^0.2.0-alpha",
      "https": "github:jspm/nodelibs-https@^0.2.0-alpha",
      "module": "github:jspm/nodelibs-module@^0.2.0-alpha",
      "os": "github:jspm/nodelibs-os@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
      "url": "github:jspm/nodelibs-url@^0.2.0-alpha",
      "util": "github:jspm/nodelibs-util@^0.2.0-alpha"
    },
    "overrides": {
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js"
        ]
      }
    }
  }
}
